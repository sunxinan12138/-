<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bhdx.DAO.AddzsMapper">
    <insert id="addZs" parameterType="com.bhdx.models.ZsDetail">
    insert into zsdetail (id,studentId,project,detail,Level,mark,imageName,img) values  (#{id}, #{studentId}, #{project}, #{detail}, #{Level}, #{mark}, #{imageName}, #{img});
    </insert>


</mapper>